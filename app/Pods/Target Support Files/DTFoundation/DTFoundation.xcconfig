CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/DTFoundation
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "$(SDKROOT)/usr/include/libxml2"
